{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid",
      "required": true
    },
    "subject": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "location": {
      "type": "string"
    },
    "externalEventURL": {
      "type": "string",
      "regex": "^https:\\/\\/([a-zA-Z0-9-]+\\.)+[a-zA-Z]{2,}(:\\d+)?(\\/[^\\s?#]*)?(\\?[^\\s#]*)?(#[^\\s]*)?$"
    },
    "datetype": {
      "type": "enumeration",
      "required": true,
      "default": "date-time",
      "enum": [
        "date-time",
        "date"
      ]
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "state": {
      "type": "enumeration",
      "required": true,
      "default": "confirmed",
      "enum": [
        "confirmed",
        "tentative",
        "cancelled"
      ]
    },
    "lastModifiedICS": {
      "type": "datetime",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "copyright": {
      "type": "component",
      "component": "shared.copyright",
      "repeatable": false
    }
  }
}
